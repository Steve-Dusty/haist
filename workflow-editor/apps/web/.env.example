# BubbleLab Workflow Editor - Environment Variables
# Copy this file to .env.local and fill in your API keys

# Google Gemini API Key (for AIAgentBubble with Gemini models)
GOOGLE_API_KEY=your_google_api_key_here

# OpenAI API Key (for AIAgentBubble with OpenAI models)
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Key (for AIAgentBubble with Claude models)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Firecrawl API Key (for WebSearchTool, WebScrapeTool, etc.)
FIRECRAWL_API_KEY=your_firecrawl_api_key_here

# Reddit Credentials (for RedditScrapeTool)
REDDIT_CLIENT_ID=your_reddit_client_id
REDDIT_CLIENT_SECRET=your_reddit_client_secret

# Slack Bot Token (for Slack integrations)
SLACK_BOT_TOKEN=your_slack_bot_token

# ===========================================
# Composio Integration
# ===========================================

# Composio API Key (from https://app.composio.dev)
COMPOSIO_API_KEY=your_composio_api_key

# Auth Config IDs (from Composio dashboard - Settings > Auth Configs)
# These are required for OAuth flows with each service
GMAIL_AUTH_CONFIG_ID=
GOOGLE_CALENDAR_AUTH_CONFIG_ID=
GOOGLE_DRIVE_AUTH_CONFIG_ID=
GOOGLE_DOCS_AUTH_CONFIG_ID=
GOOGLE_SHEETS_AUTH_CONFIG_ID=
SLACK_AUTH_CONFIG_ID=
NOTION_AUTH_CONFIG_ID=
GITHUB_AUTH_CONFIG_ID=
OUTLOOK_AUTH_CONFIG_ID=
